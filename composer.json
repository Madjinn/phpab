{
	"name": "phpab/phpab",
	"type": "library",
	"description": "A PHP library to create A/B tests.",
	"keywords": [
		"Phpab",
		"Ab test",
		"abtest"
	],
	"homepage": "https://github.com/phpab/phpab",
	"license": "MIT",
	"require": {
		"php": "~5.4"
	},
	"autoload": {
		"psr-4": {
			"Phpab\\Phpab\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Phpab\\Phpab\\": "tests"
		}
	},
	"scripts": {
		"test": "phpunit"
	}
}